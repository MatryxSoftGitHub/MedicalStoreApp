function onClick(){            
    document.getElementById("sellDetailsTable").style.display="block";         
    myHtmlContent = "<tr><td><a  type='button' class='btn btn-success' onclick='addRow()'>+</a></td><td><input type='text' id='medicine_name' name='medicine_name' class='form-control' oninput='onMedInput()' list='getMedicine'><datalist id='getMedicine'><% for(var i=0; i<getMedicine.length; i++) { %><option id='<%= getMedicine[i]._id %>' value='<%= getMedicine[i].item_name %>'><%= getMedicine[i].item_name %></option><% } %></datalist></td><td><input type='number' id='total_quantity' name='total_quantity' class='form-control' readonly></td><td><input type='number' id='unit_price' name='unit_price' class='form-control' readonly></td><td><input type='number' id='sell_quantity' name='sell_quantity' oninput='changeQty()' class='form-control'></td><td><input type='number' id='total_amount' name='total_amount' class='form-control' readonly></td><td><a type='button' class='btn btn-success' onclick='deleteRow(this)'>Delete</a></td></tr>"
    var tableRef = document.getElementById('sellDetailsTable').getElementsByTagName('tbody')[0];
    var newRow = tableRef.insertRow(tableRef.rows.length);
    newRow.innerHTML = myHtmlContent;
}

function addRow() {
    //var myHtmlContent = "<tr><td><a  type='button' class='btn btn-success' onclick='addRow()'>+</a></td><td><input type='text' id='medicine_name' name='medicine_name' class='form-control' oninput='onMedInput()' list='getMedicine'><datalist id='getMedicine'><% for(var i=0; i<getMedicine.length; i++) { %><option id='<%= getMedicine[i]._id %>' value='<%= getMedicine[i].item_name %>'><%= getMedicine[i].item_name %></option><% } %></datalist></td><td><input type='number' id='total_quantity' name='total_quantity' class='form-control' readonly></td><td><input type='number' id='unit_price' name='unit_price' class='form-control' readonly></td><td><input type='number' id='sell_quantity' name='sell_quantity' oninput='changeQty()' class='form-control'></td><td><input type='number' id='total_amount' name='total_amount' class='form-control' readonly></td><td><a type='button' class='btn btn-success' onclick='deleteRow(this)'>Delete</a></td></tr>"
    var tableRef = document.getElementById('sellDetailsTable').getElementsByTagName('tbody')[0];
    var newRow = tableRef.insertRow(tableRef.rows.length);
    newRow.innerHTML = myHtmlContent;         
}

function onMedInput() {
    medicineName = document.getElementById("medicine_name").value;
    alert(medicineName)
    selectedMedOptionId = $('#getMedicine').find('option[value="' + medicineName + '"]').attr('id');
    alert(selectedMedOptionId)
    var allMedicines = <%- JSON.stringify(getMedicine) %>;
    for(let i=0;i<allMedicines.length;i++){
        if(allMedicines[i]._id==selectedMedOptionId){
            alert('Matched')
            document.getElementById("total_quantity").value = allMedicines[i].quantity
            alert(allMedicines[i].quantity)
            var unitPrice = (allMedicines[i].unit_price).split(" ")
            alert(unitPrice)
            document.getElementById("unit_price").value=unitPrice[0]
            alert(unitPrice[0])
            break
        }
    }            
}

function changeQty() {
    var mediQty = document.getElementById("total_quantity").value;
    var sellQty = document.getElementById("sell_quantity").value;
    var mediPrice = document.getElementById('unit_price').value;        
    var totalAmt = ((sellQty)*(mediPrice));
    if(totalAmt == 0){
        document.getElementById('total_amount').value = "$$$$$$$$";
    } else {
        document.getElementById('total_amount').value = totalAmt;
    }
    outSellQty = sellQty;
}


